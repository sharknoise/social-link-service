from fastapi import FastAPI

app = FastAPI(
    {% if info %}
    {% for key,value in info.items() %}
    {% if key != "servers" %}
    {{ key }} = "{{ value }}",
    {% else %}
    {{ key }} = {{ value }},
    {% endif %}
    {% endfor %}
    {% endif %}
    docs_url="/api/docs",
    redoc_url="/api/redoc",
    openapi_url="/api/v1/communications/openapi.json",
    )
